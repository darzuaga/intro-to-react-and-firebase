pet","generated":{"line":74,"column":15},"source":"components/PostSnippet.jsx","original":{"line":59,"column":15}},{"name":"PostSnippet","generated":{"line":86,"column":26},"source":"components/PostSnippet.jsx","original":{"line":6,"column":6}}],"sources":{"components/PostSnippet.jsx":"import React from 'react'\nimport { Card } from 'antd';\nimport { Link } from \"@reach/router\";\nimport db from '../firebase'\n\nconst PostSnippet = (props) => {\n\n    const onDeletePost = () => {\n        console.log('post being deleted')\n        let postRef = db\n            .collection('users')\n            .doc(props.user.uid)\n            .collection('posts')\n            .doc(props.id)\n\n        postRef.delete()\n    }\n\n    return (\n        <div className=\"post_snippet_container\">\n            <Card\n                style={{ marginTop: 16 }}\n                type=\"inner\"\n                title={props.title}\n                extra={\n                    <div class=\"post_snippet_links\">\n                        <Link to={`/blogs/${props.uid}/post/${props.id}`} style={{ marginRight: '15px', float: 'left' }}>\n                            Read Full Article\n                        </Link>\n\n                        {props.user &&\n                            <div className=\"post_edit_links\" style={{ float: 'right' }}>\n                                <Link to={`/update_post/${props.id}`} style={{ marginRight: '15px' }}>\n                                    Edit\n                                </Link>\n\n                                <a onClick={onDeletePost}>\n                                    Delete\n                                </a>\n                            </div>\n                        }\n                    </div>\n                }\n            >\n\n                <p className=\"article_content\">\n                    {\n                        props.content.split('\\n').map((paragraph, idx) => {\n                            return <p key={idx}>{paragraph}</p>;\n                        })\n                    }\n                </p>\n\n            </Card>\n        </div>\n    )\n}\n\nexport default PostSnippet;"},"lineCount":null}},"error":null,"hash":"714919d85468a629128ccf86ab5eb21c","cacheData":{"env":{}}}